{
  "name": "C++",
  "image": "omzn/kit-lpp:multiple",
  "onCreateCommand": "cp ${containerWorkspaceFolder}/.devcontainer/greeting.fish $HOME/.config/fish/conf.d/",
  "features": {
    "ghcr.io/meaningful-ooo/devcontainer-features/fish:1": {
      "fisher": true
    },
    "ghcr.io/devcontainers/features/github-cli:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cmake-tools",
        "ms-vscode.cpptools",
        "llvm-vs-code-extensions.vscode-clangd",
        "eamodio.gitlens"
      ]
    }
  }
}
